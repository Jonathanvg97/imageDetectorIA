{
  "name": "imagedetectoria",
  "version": "1.0.0",
  "description": "Image Detector IA Backend",
  "main": "src/app.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node src/config/initDb.ts && cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/app.ts",
    "dev:test": "cross-env NODE_ENV=test ts-node src/config/initDb.ts && cross-env NODE_ENV=test ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "db:init": "ts-node src/config/initDb.ts",
    "start": "cross-env NODE_ENV=production node dist/app.js",
    "test:init": "dotenv -e .env.test ts-node src/config/initDb.ts",
    "test": "npm run test:init && dotenv -e .env.test jest --coverage",
    "test:watch-unit": "jest --watch --testPathPattern=test/unit",
    "test:watch-integration": "jest --watch --testPathPattern=test/integration",
    "test-e2e": "npm run test:init && playwright test --ui && npx playwright show-report"
  },
  "keywords": [
    "image",
    "detector",
    "ia"
  ],
  "author": "Jonathan Vanegas",
  "license": "ISC",
  "dependencies": {
    "@huggingface/inference": "^2.8.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/nodemailer": "^6.4.16",
    "@types/uuid": "^10.0.0",
    "ajv": "^8.17.1",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.20.0",
    "google-auth-library": "^9.14.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.15",
    "pg": "^8.12.0",
    "punycode": "^2.3.1",
    "ts-node": "^10.9.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.2",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "@types/pg": "^8.11.9",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "playwright": "^1.47.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  }
}
