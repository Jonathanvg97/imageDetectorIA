{
  "name": "imagedetectoria",
  "version": "1.0.0",
  "description": "Image Detector IA Backend",
  "main": "src/app.ts",
  "scripts": {
    "migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev --name init",
    "seed": "dotenv -e .env.development ts-node prisma/seed.ts",
    "migrate-and-seed": "npm run migrate:dev && npm run seed",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc && npm run start",
    "start": "cross-env NODE_ENV=production node dist/app.js"
  },
  "keywords": [
    "image",
    "detector",
    "ia"
  ],
  "author": "Jonathan Vanegas",
  "license": "ISC",
  "dependencies": {
    "@huggingface/inference": "^2.8.0",
    "@prisma/client": "^5.19.1",
    "@types/cookie-parser": "^1.4.7",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "google-auth-library": "^9.14.1",
    "morgan": "^1.10.0",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.4",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "prisma": "^5.19.1",
    "ts-node-dev": "^2.0.0"
  }
}
