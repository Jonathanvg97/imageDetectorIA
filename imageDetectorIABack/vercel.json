{
  "version": 2,
  "builds": [
    {
      "src": "src/app.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/src/app.js"
    }
  ],
  "build": {
    "commands": {
      "build": "prisma generate"
    }
  }
}
