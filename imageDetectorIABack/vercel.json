{
  "version": 2,
  "builds": [
    {
      "src": "src/app.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/src/app.js"
    }
  ],
  "build": {
    "env": {
      "DATABASE_URL": "@database-url"
    }
  }
}
